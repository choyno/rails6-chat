<p>Youtube Link: https://www.youtube.com/watch?v=nRP91C1uX-w</p>
<p>Find me in app/views/landing/index.html.erb</p>

<div id="msg">
  <% @messages.each do |message| %>
    <div class="message">
      <span style="color:red"><%= message.user.try(:username) %></span>:
        <%= message.content %>
    </div>
  <% end %>
</div>

<%= form_for @message, remote: true do |f| %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
